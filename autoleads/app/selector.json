{
    "amazon": {
      "title": [
        {
          "selector": "one",
          "select": "#productTitle"
        },
        {
          "selector": "one",
          "select": "title",
          "slice": "Amazon.com: "
        }
      ],
      "brand": [
        {
          "selector": "one",
          "select": "#bylineInfo_feature_div .a-link-normal",
          "slice": [
            "Brand: ",
            "Visit the "
          ]
        }
      ],
      "price": [
        {
          "selector": "one",
          "select": ".priceToPay .a-offscreen"
        },
        {
          "selector": "one",
          "select": "#corePrice_desktop .a-price .a-offscreen"
        },
        {
          "selector": "one",
          "select": "#buyBoxAccordion #subscriptionPrice span"
        }
      ],
      "reviews": [
        {
          "selector": "one",
          "select": "[data-asin=\"B07N7PK9QK\"] #acrCustomerReviewLink span",
          "slice": [
            " ratings"
          ]
        },
        {
          "selector": "one",
          "select": "#productDetails_feature_div #acrCustomerReviewLink span",
          "slice": [
            " ratings"
          ]
        }
      ],
      "ratings": [
        {
          "selector": "one",
          "select": "[data-asin=\"B07N7PK9QK\"] .a-color-base"
        }
      ],
      "image": [
        {
          "selector": "one",
          "select": "#imageBlock #imgTagWrapperId img",
          "key": "src"
        },
        {
          "selector": "one",
          "select": "[data-asin=\"B07N7PK9QK\"] #comparison_image",
          "key": "data-src"
        }
      ],
      "bought": [
        {
          "selector": "one",
          "select": "#socialProofingAsinFaceout_feature_div .social-proofing-faceout-title-text span",
          "slice": [
            " bought in past week"
          ]
        }
      ],
      "in_stock": [
        {
          "selector": "one",
          "select": "#outOfStock",
          "none": null
        }
      ],
      "sold_by": [
        {
          "selector": "one",
          "select": "#buyBoxAccordion #addToCart #desktop_qualifiedBuyBox #sellerProfileTriggerId"
        },
        {
            "selector": "one",
            "select": "#CardInstanceWKQzoMTiqN79Q_3rYoUicQ ._posts-dp-same-brand-desktop-card_style_post-carousel-brand-name__FFo4L span"
        },
        {
            "selector": "all",
            "select": "#buyBoxAccordion #sfsb_accordion_head div div span",
            "for": "span",
            "slice": [
              "\n",
              ":"
            ]
        }
      ],
      "details": [
        {
          "selector": "all",
          "select": "#detailBulletsWrapper_feature_div ul li span",
          "for": "span",
          "slice": [
            "\n",
            ":"
          ],
          "ignore_after": "Reviews"
        },
        {
          "selector": "all",
          "select": "#productDetails_feature_div #productDetails_detailBullets_sections1 tbody",
          "for": "tr",
          "slice": [
            "\n",
            ":"
          ],
          "ignore_after": "Reviews"
        }
      ],
      "url": [],
      "asin": []
    },
    "google_search": {
        "keys": ["url", "price"],
        "base_url": "https://www.google.com/search?hl=en&gl=us&q=",
